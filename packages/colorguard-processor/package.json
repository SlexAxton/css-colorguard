{
  "name": "colorguard-processor",
  "version": "1.0.48",
  "description": "PostCSS processor to keep a watchful eye on your css colors",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "yarn run build",
    "build": "tsup",
    "test": "tape test/*.ts | tap-spec"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bryanjtc/css-colorguard-upgraded.git"
  },
  "keywords": [
    "css",
    "colors",
    "lint",
    "csslint",
    "postcss",
    "colorguard",
    "postcss-processor"
  ],
  "author": "Bryan Thomas <49354825+bryanjtc@users.noreply.github.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bryanjtc/css-colorguard-upgraded/issues"
  },
  "homepage": "https://github.com/bryanjtc/css-colorguard-upgraded/tree/master/packages/colorguard-processor/README.md",
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.6",
    "@semantic-release/npm": "11.0.0",
    "@types/node": "18.17.17",
    "@types/postcss-reporter": "7.0.1",
    "@types/tape": "5.6.1",
    "@types/text-table": "0.2.3",
    "@w4rlock/semantic-release-monorepo": "1.0.3",
    "colorette": "2.0.20",
    "gitmojis": "3.13.5",
    "log-symbols": "5.1.0",
    "plur": "5.1.0",
    "postcss": "8.4.29",
    "postcss-colorguard": "workspace:*",
    "postcss-reporter": "7.0.5",
    "semantic-release": "22.0.0",
    "semantic-release-gitmoji": "1.6.4",
    "tap-spec": "5.0.0",
    "tape": "5.6.6",
    "text-table": "0.2.0",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@3.6.3"
}
