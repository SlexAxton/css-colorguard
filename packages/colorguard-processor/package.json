{
  "name": "colorguard-processor",
  "version": "0.0.0",
  "description": "PostCSS processor to keep a watchful eye on your css colors",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "tape test/*.ts | tap-spec"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "colorguard": "./dist/cli"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bryanjtc/css-colorguard.git"
  },
  "keywords": [
    "css",
    "colors",
    "lint",
    "csslint",
    "postcss",
    "colorguard",
    "postcss-processor"
  ],
  "author": "Bryan Thomas <49354825+bryanjtc@users.noreply.github.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bryanjtc/css-colorguard/issues"
  },
  "homepage": "https://github.com/bryanjtc/css-colorguard/tree/master/packages/colorguard-processor/README.md",
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/postcss-reporter": "^7.0.1",
    "@types/tape": "^4.13.2",
    "@types/text-table": "^0.2.2",
    "@types/yargs": "^17.0.17",
    "colorette": "2.0.19",
    "log-symbols": "^5.1.0",
    "plur": "^5.1.0",
    "postcss": "^8.4.20",
    "postcss-colorguard": "workspace:^",
    "postcss-reporter": "^7.0.5",
    "tap-spec": "^5.0.0",
    "tape": "^5.6.1",
    "text-table": "^0.2.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "packageManager": "yarn@3.3.0"
}
