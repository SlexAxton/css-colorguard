{
  "name": "postcss-colorguard",
  "version": "0.0.0",
  "description": "PostCSS plugin to keep a watchful eye on your css colors",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bryanjtc/css-colorguard.git"
  },
  "keywords": [
    "css",
    "colors",
    "lint",
    "csslint",
    "postcss",
    "colorguard",
    "postcss-plugin"
  ],
  "author": "Bryan Thomas <49354825+bryanjtc@users.noreply.github.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bryanjtc/css-colorguard/issues"
  },
  "homepage": "https://github.com/bryanjtc/css-colorguard/tree/master/packages/postcss-colorguard/README.md",
  "peerDependencies": {
    "postcss": ">8.0.0"
  },
  "devDependencies": {
    "@types/color-diff": "1.2.1",
    "@types/color-rgba": "2.1.0",
    "color-diff": "1.2.0",
    "color-rgba": "2.4.0",
    "postcss": "8.4.20",
    "synesthesia": "1.0.1",
    "tsup": "6.5.0",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "packageManager": "yarn@3.3.0"
}
