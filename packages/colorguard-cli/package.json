    "lint",
    "csslint",
    "postcss",
    "colorguard",
    "cli"
  ],
  "author": "Bryan Thomas <49354825+bryanjtc@users.noreply.github.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bryanjtc/css-colorguard-upgraded/issues"
  },
  "homepage": "https://github.com/bryanjtc/css-colorguard-upgraded/tree/master/packages/colorguard-cli/README.md",
  "devDependencies": {
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.7",
    "@semantic-release/npm": "9.0.2",
    "@types/node": "18.11.18",
    "@types/yargs": "17.0.17",
    "colorguard-processor": "workspace:*",
