{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "strict": true,
    "incremental": false,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "skipLibCheck": true,
    "target": "es2020",
    "moduleResolution": "node"
  }
}
